--TEST--
"domain" function
--TEMPLATE--
domain: {{ domain(host).content }}
subDomain : {{ domain(host).subDomain }}
registrableDomain : {{ domain(host).registrableDomain }}
publicSuffix : {{ domain(host).publicSuffix }}
isICANN : {{ domain(host).ICANN ? 'ok' : 'ko' }}
isPrivate : {{ domain(host).private ? 'ok' : 'ko' }}
isKnown : {{ domain(host).known ? 'ok' : 'ko' }}
ascii : {{ domain(host).toAscii.content }}
